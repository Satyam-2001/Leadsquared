{
  "name": "leadsquared-sdk",
  "version": "0.0.1",
  "description": "SDK for LeadSquare integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "prepublishOnly": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "leadsquared",
    "sdk"
  ],
  "author": "Satyam Lohiya",
  "license": "ISC",
  "packageManager": "pnpm@10.6.3",
  "devDependencies": {
    "@types/node": "^22.15.23",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}
